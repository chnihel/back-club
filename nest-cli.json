{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      {
        "include": "public/**/*",
        "outDir": "dist/public"
      },
      {
        "include": "storage/**/*",
        "outDir": "dist/storage"
      }
    ]
  }
}
